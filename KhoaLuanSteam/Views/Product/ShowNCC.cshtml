@model IEnumerable<KhoaLuanSteam.Models.NHACUNGCAP>


<ul class="list-group list-group-flush text-left">
    @{
        foreach (var item in Model)
        {
            <li class="list-group-item" style="padding: 10px 30px;">
                <a href="@Url.Action("SPTheoNCC", "Product", new { @maNCC = @item.MaNCC })" style="text-transform:uppercase;text-decoration:none ;
   font-family: Roboto,HelveticaNeue,Helvetica Neue,sans-serif ; font-weight:400;line-height:1.7 ">
                    @item.TenNCC
                </a>
            </li>
        }

    }
</ul>
